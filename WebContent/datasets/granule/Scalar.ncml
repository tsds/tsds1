<?xml version="1.0" encoding="UTF-8"?>
<netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2"  
    location="Scalar.txt" iosp="lasp.tss.iosp.AsciiGranuleReader">


    <attribute name="title" value="Single variable time series" />

    <dimension name="time" isUnlimited="true"/> 

    <variable name="time" shape="time" type="String" column="1 2">
        <attribute name="units" type="String" value="yy MM" />
    </variable>

    <!--variable name="v1" shape="time" type="double" column="2"/-->
    <variable name="v2" shape="time" type="double" column="3"/>
    <variable name="v3" shape="time" type="double" column="4"/>
    <variable name="v4" shape="time" type="double" column="5"/>
    <variable name="v5" shape="time" type="String" column="6"/>
    
</netcdf>
